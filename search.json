---
layout: null
permalink: /search.json
---
[
  {% for blog in site.blogs %}
    {
      "title"    : "{{ blog.title | escape }}",
      "category" : "{{ blog.category }}",
      "tags"     : "{{ blog.tags | join: ', ' }}",
      "url"      : "{{ blog.external_url | default: blog.url | relative_url }}",
      "date"     : "{{ blog.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]